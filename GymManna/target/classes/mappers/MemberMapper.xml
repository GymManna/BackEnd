<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="myMember">

   <resultMap id="aa"
      type="com.mycompany.mygym.member.vo.Member">
      <result property="memberId" column="memberId" jdbcType="VARCHAR"
         javaType="String" />
      <result property="memberPw" column="memberPw" jdbcType="VARCHAR"
         javaType="String" />
      <result property="memberName" column="memberName" />      
   </resultMap>

   <select id="LoginMember" resultMap="aa">
      select memberId,memberPw,memberName
      from members
      WHERE memberId = #{memberId} AND memberPw = #{memberPw}
   </select>

</mapper>